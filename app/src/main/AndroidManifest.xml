<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rseu.final_qualifying_work"
    >
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:name=".RealmApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_ktiib"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_ktiib"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".screens.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".screens.MainActivity"
            android:parentActivityName=".screens.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            >
            <intent-filter>

                <action android:name="android.intent.action.MAIN" />

            </intent-filter>
        </activity>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>